{
  "extends": [
    "xo/esnext",
    "prettier"
  ],
  "rules": {
    "camelcase": 0,
    "consistent-return": 0,
    "indent": [2, 2],
    "no-new": 0,
    "no-trailing-spaces": 0,
    "prefer-reflect": 0,
    "semi": [1, "never"],
    "space-before-function-paren": [2, "never"],
    "capitalized-comments": 0
  },
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "emcaFeatures": {
    "modules": true
  }
}
